{
  "name": "openlawnz-search",
  "version": "1.0.0",
  "description": "API for openlawnz search engine",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "rm -rf .tmp && mkdir .tmp && mkdir -p dist && babel index.js -o .tmp/index.js --presets @babel/env && cp package.json .tmp/ && zip dist/dist.zip .tmp/* -j && rm -rf .tmp",
    "dev": "rm -rf debug && mkdir debug && babel index.js -o debug/index.js --presets @babel/env && node -r dotenv/config debug/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openlawnz/openlawnz-search.git"
  },
  "author": "Ed Haslam",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/openlawnz/openlawnz-search/issues"
  },
  "homepage": "https://github.com/openlawnz/openlawnz-search#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "dotenv": "^8.0.0"
  },
  "resolutions": {
    "lodash": "^4.17.14"
  }
}
